#/bin/sh
cells component:lint
R=$?
if [ $R != 0 ]
        then
	echo "Error found in lint task with exit code " $R
        exit $R
fi

cells component:coverage
R=$?
if [ $R != 0 ]
        then
	echo "Error found in coverage task with exit code " $R
        exit $R
fi

cells component:test-demo
R=$?
if [ $R != 0 ]
        then
	echo "Error found in demo check task with exit code " $R
        exit $R
fi
